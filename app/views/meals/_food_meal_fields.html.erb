<li class="control-group nested-fields">
  <div class="controls">
    <%= f.label :quantity, "Amount:" %>
    <%= f.text_field :quantity %>

    <%= f.select :food , Food.all.map { |food| [food.name, food.id]}, {}, {class: 'multiselect', multiple: true} %>

    <%= link_to_remove_association "remove", f %>
  </div>
</li>